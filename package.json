{
  "name": "stackoverflow-lite",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "yarn run clean && tsc",
    "clean": "rm -rf dist",
    "test:staging": "cross-env NODE_ENV=staging mocha -r ts-node/register test/**/*.test.ts --exit",
    "test": "cross-env NODE_ENV=development mocha -r ts-node/register test/**/*.test.ts --exit",
    "start": "cross-env NODE_ENV=development node dist/server.js",
    "start:dev": "cross-env NODE_ENV=development ts-node server.ts",
    "lint": "eslint ./ --fix",
    "migration:generate": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:generate",
    "migration:run:dev": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:run:staging": "cross-env NODE_ENV=staging ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:revert",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "expect": "^23.4.0",
    "express": "^4.16.3",
    "lodash": "^4.17.19",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.4",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^3.1.0",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.14",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
